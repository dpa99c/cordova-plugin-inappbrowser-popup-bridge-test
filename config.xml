<?xml version='1.0' encoding='utf-8'?>
<widget id="cordova-popupbridge-test" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PopupBridge Test</name>
    <description>
    </description>
    <author>Dave Alden</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-popupbridge" spec="https://github.com/dpa99c/cordova-plugin-popupbridge.git" />
    <plugin name="cordova-plugin-wkwebview-engine" spec="https://github.com/dpa99c/cordova-plugin-wkwebview-engine#popup_bridge" />
</widget>
